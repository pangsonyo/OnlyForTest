<?xml version="1.0"?>
<Rules>
    <rule expression="i%3==0" BehaviorClass="com.impl.BuzzPredicate">
        <property name="isUse" value="true"/>
        <property name="isDefault" value="false"/>
        <property name="order" value="1"/>
    </rule>

    <rule expression="i%5==0" BehaviorClass="com.impl.FizzPredicate">
        <property name="isUse" value="true"/>
        <property name="isDefault" value="false"/>
        <property name="order" value="1"/>
    </rule>

    <Bean id="BuzzPredicate" BehaviorClass="com.impl.BuzzPredicate" >
        <property name="isUse" value="true"/>
        <property name="isDefault" value="false"/>
        <property name="order" value="1"/>
    </Bean>

</Rules>